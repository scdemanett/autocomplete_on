{
   "content_scripts": [ {
      "js": [ "autocomplete.js" ],
      "matches": [ "http://*/*", "https://*/*" ],
      "run_at": "document_start"
   } ],
   "description": "Changes 'autocomplete=off' to 'autocomplete=on' in web pages, so your passwords will be remembered.",
   "icons": {
      "16": "icon16.png"
      "32": "icon32.png"
      "48": "icon48.png"
      "128": "icon128.png"
   },
   "name": "Autocomplete On",
   "version": "1.0",
    "manifest_version": 2
}
